{
    "manifest_version": 3,
    "name": "Auto Tab Closer",
    "version": "1.1",
    "description": "Closes tabs with certain URLs after some time.",
    "permissions": ["tabs"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_title": "Auto Tab Closer"
    }
  }
  
